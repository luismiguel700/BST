ocamllex  lexer.mll
ocamlyacc  parser.mly
ocamlc -c -dtypes types.mli
ocamlc -c -dtypes types.ml
ocamlc -c -dtypes comm.mli
ocamlc -c -dtypes comm.ml
ocamlc -c -dtypes parser.mli
ocamlc -c -dtypes parser.ml
ocamlc -c -dtypes lexer.ml
ocamlc -c -dtypes main.ml
ocamlc -o main types.cmo lexer.cmo parser.cmo main.cmo
